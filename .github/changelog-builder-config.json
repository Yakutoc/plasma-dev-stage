{
    "ignore_labels": ["changelog-skip"],
    "pr_template": "#{{DESC}}\n#{{TITLE}} (#{{URL}})\n",
    "custom_placeholders": [
        {
            "name": "DESC",
            "source": "BODY",
            "transformer": {
                "pattern": "(?:\n### What/why changed)(.*)",
                "flags": "s",
                "target": " "
            }
        }
    ]
}
